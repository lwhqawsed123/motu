.record {
  width: 100%;
  min-height: calc(88vh);
  box-sizing: border-box;
  background-color: #e6e8eb;
  padding-bottom: 30px;

  .cursor {
    cursor: pointer;
  }
  
  .verify_status {
    display: inline-block;
    padding: 4px;
    background: #feb3c5;
    color: #fff;
    border-radius: 3px;
    font-size: 12px;
  }

  .icon-gender-men {
    color: #9acd32;
  }

  .icon-gender-women {
    color: #eead97;
  }

  .visitor-info-name {
    color: #fe0b0e;
    display: inline-block;
    word-break: break-all;
    text-align: center;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    // display: -webkit-box;
    /*自适应盒子*/
    -webkit-line-clamp: 1;
    /*此处为1行,如果是两行就改成2*/
    -webkit-box-orient: vertical;
  }
  .gender-men{
    display: inline-block;
    background-color: #8ca3f0;
    color: #ffffff;
    padding: 4px;
    font-size: 12px;
    border-radius: 4px ;
    line-height: 12px;
    box-sizing: border-box;
    font-family: "Open Sans", sans-serif;
  }
  .gender-women{
    background-color: #feb3c5;

  }

  .maikefeng {
    color: #ff69b4;
  }

  .icon-size {
    font-size: 24px;
    font-weight: 600;
  }

  .icon-right-color {
    color: #ff0000;
  }

  .icon-back-color {
    color: #008000;
  }

  .icon-fail-color {
    color: #808080;
  }

  .anchor-info {
    text-align: center;
    padding-top: 3px;

    .profile-box {
      margin: 0 auto;
      position: relative;
      width: 50px;
      height: 50px;
      border-radius: 5px;
      margin-bottom:3px;

      .profile {
        width: 100%;
        height: 100%;
        border-radius: 5px;
      }

      // .profile-status {
      //   width: 8px;
      //   height: 8px;
      //   border-radius: 50%;
      //   border: 1px solid #ffffff;
      //   background-color: #dcdcdc;
      //   position: absolute;
      //   top: -3px;
      //   right: -3px;
      //   box-sizing: border-box;
      // }

      // .outline {
      //   background-color: #9acd32;
      // }
    }

    .anchor-name {
      display: flex;
      justify-content: center;

      .icon-gender-men {
        color: #9acd32;
      }

      .icon-gender-women {
        color: #eead97;
      }

      .visitor-info-name {
        color: #fe0b0e;
        display: inline-block;
        word-break: break-all;
        text-align: center;
        text-overflow: -o-ellipsis-lastline;
        overflow: hidden;
        text-overflow: ellipsis;
        // display: -webkit-box;
        /*自适应盒子*/
        -webkit-line-clamp: 1;
        /*此处为1行,如果是两行就改成2*/
        -webkit-box-orient: vertical;
      }

      .maikefeng {
        color: #ff69b4;
      }

      .is-not-liveing {
        color: #cccccc;
      }
    }
  }

  .record-conter {
    min-height: calc(88vh);
    background-color: #fff;
    border-radius: 5px;
    position: relative;
    padding-bottom: 100px;

    .record-top {
      margin-left: 0;
      margin-right: 0;
      min-height: 60px;
      padding-bottom: 10px;
      border-bottom: 1px solid #d6d6d6;

      .el-form--inline .el-form-item {
        margin-top: 10px;
        margin-bottom: 0;

        .el-input {
          width: 140px;
        }
      }
    }

    .addrecord {
      width: 120px;
      height: 36px;
      margin-top: 10px;
      background-color: #ef8200;
      border: 1px solid #d1780f;
      text-align: center;
      line-height: 36px;
      color: #fff;
      font-size: 14px;
      cursor: pointer;
    }

  }

  .owner-dialog-footer {
    .owner-btn {
      width: 82px;
      height: 32px;
      border-radius: 2px;
      text-align: center;
      line-height: 32px;
      font-weight: bold;
      display: inline-block;
      font-size: 14px;
    }

    .btn,
    .btn1 {
      background: #169bd5;
      border: 1px solid #169bd5;
      color: #fff;
    }

    .btn2 {
      border: 1px solid #cccccc;
      color: #000000;
    }
  }

  .record-dialog {
    p {
      font-size: 14px;
      color: #656565;
      height: 40px;

      span {
        margin-left: 60px;
      }
    }

    label {
      font-weight: normal;
    }

    p.record-zhu {
      font-size: 12px;
      color: #b0b0b0;
    }
  }
}